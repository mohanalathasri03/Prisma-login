generator client {
    provider = "prisma-client-js"
  }

  datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
  }

  model User {
    id        Int      @id @default(autoincrement())
    email     String   @unique
    password  String?
googleId  String?  @unique
    role      Role
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
  }

  enum Role {
    USER
    ADMIN
  }